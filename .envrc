#!/bin/bash

if [ -f ~/.githooks/coveralls.gpg ]; then
    COVERALLS_REPO_TOKEN="$(gpg -d -q "$HOME"/.githooks/coveralls.gpg)"
    export COVERALLS_REPO_TOKEN
fi
ANSIBLE_VAULT_PASSWORD_FILE=/etc/ansible/vault
DOCKER_REPO=edwardtheharris/ethereum-node
GHCR_REPO=ghcr.io/edwardtheharris/helm-ethereum-node/node
GNUPGHOME=$(pwd)/.gnupg
PASSHOLE_DB="${HOME}/.local/share/passhole/ethereum.kdbx"
PIPENV_VENV_IN_PROJECT=1
VERSION="0.0.1"

export ANSIBLE_VAULT_PASSWORD_FILE
export DOCKER_REPO
export GHCR_REPO
export GNUPGHOME
export PASSHOLE_DB
export PIPENV_VENV_IN_PROJECT
export VERSION
